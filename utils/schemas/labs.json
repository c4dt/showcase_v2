            "name": {
              "type": "string"
            },
            "prof": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "email": {
                  "type": "string"
                }
              },
              "required": ["name", "email"]
            },
            "description": {
              "type": "string"
            },
            "url": {
              "type": "string"
            },
            "contacts": {
              "type": "array",
