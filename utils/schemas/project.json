                "label": {
                  "type": "string"
                },
                "text": {
                  "type": "string"
                },
                "url": {
                  "type": "string"
                }
              },
              "required": ["text"]
            }
          }
        },
        "required": ["type", "url", "title"]
      }
    },
    "maturity": {
      "type": "integer"
    },
    "incubator": {
      "type": "object",
      "properties": {
        "work": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": ["incubated", "incubated_market", "retired", "retired_archived"],
          "default": "incubated"
        },
        "products": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "Demo",
                  "Hands-on",
                  "Pilot",
                  "App",
                  "Library",
                  "OSS-participation",
                  "Archive",
                  "Project",
                  "Blog"
                ]
              },
              "url": {
                "type": "string"
              },
              "title": {
                "type": "string"
              },
              "code": {
                "type": "string"
              }
            },
            "required": ["type", "url", "title"]
          }
        }
      },
      "required": ["work"]
    },
    "date_updated": {
      "type": "string"
    }
